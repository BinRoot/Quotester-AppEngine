<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>LogIn</servlet-name>
		<servlet-class>com.binroot.LogInServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LogIn</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>QueryQuotes</servlet-name>
		<servlet-class>com.binroot.QueryQuoteServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>QueryQuotes</servlet-name>
		<url-pattern>/quotes</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>PostQuote</servlet-name>
		<servlet-class>com.binroot.PostQuoteServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PostQuote</servlet-name>
		<url-pattern>/post</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>AddTarget</servlet-name>
		<servlet-class>com.binroot.AddTargetServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddTarget</servlet-name>
		<url-pattern>/add</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>TargetList</servlet-name>
		<servlet-class>com.binroot.TargetListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TargetList</servlet-name>
		<url-pattern>/list</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>AdminServlet</servlet-name>
		<servlet-class>com.binroot.AdminServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AdminServlet</servlet-name>
		<url-pattern>/admin</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>DeleteQuote</servlet-name>
		<servlet-class>com.binroot.DeleteQuoteServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeleteQuote</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>
	
	
	<welcome-file-list>
		<welcome-file>list</welcome-file>
	</welcome-file-list>
</web-app>
